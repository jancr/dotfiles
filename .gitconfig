# This is Git's per-user configuration file.
[user]
	name = "Jan Christian Refsgaard"
	email = jcr@gmail.com
[core]
	excludesfile = $HOME/.config/.gitignore_global
	eol = native
	autocrlf = input
[push]
	default = matching

[includeIf "gitdir:/novo/jref/"]
	path = ~/.conf/.gitworkconfig
[includeIf "gitdir:/home/jref/"]
	path = ~/.conf/.gitworkconfig
